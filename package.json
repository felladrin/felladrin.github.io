{
  "name": "victornogueira.app",
  "version": "2.0.0",
  "private": true,
  "type": "module",
  "source": "./src/index.html",
  "scripts": {
    "dev": "parcel",
    "build": "parcel build --public-url ./ --no-source-maps && echo 'www.victornogueira.app' > ./dist/CNAME",
    "prebuild": "node -e \"fs.rmSync('dist', { recursive: true })\"",
    "cypress-open": "cypress open",
    "cypress-run": "cypress run",
    "test": "npm-run-all --parallel --race dev cypress-run"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "6.1.2",
    "@fortawesome/free-brands-svg-icons": "6.1.2",
    "@fortawesome/free-solid-svg-icons": "6.1.2",
    "create-pubsub": "1.4.0",
    "halfmoon": "1.1.1",
    "winbox": "0.2.4"
  },
  "devDependencies": {
    "@parcel/transformer-inline-string": "2.7.0",
    "@types/halfmoon": "1.0.2",
    "@types/winbox": "0.2.1",
    "cypress": "10.6.0",
    "npm-run-all": "4.1.5",
    "parcel": "2.7.0",
    "typescript": "4.7.4",
    "vconsole": "3.14.6"
  },
  "alias": {
    "winbox": "winbox/src/js/winbox.js"
  }
}
